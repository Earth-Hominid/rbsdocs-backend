{
  "kind": "collectionType",
  "collectionName": "reports",
  "info": {
    "name": "reports"
  },
  "options": {
    "increments": true,
    "timestamps": true,
    "draftAndPublish": true
  },
  "pluginOptions": {},
  "attributes": {
    "title": {
      "type": "string"
    },
    "description": {
      "type": "richtext"
    },
    "content": {
      "type": "richtext"
    },
    "image": {
      "model": "file",
      "via": "related",
      "allowedTypes": [
        "images",
        "files",
        "videos"
      ],
      "plugin": "upload",
      "required": false,
      "pluginOptions": {}
    },
    "slug": {
      "type": "uid",
      "targetField": "title"
    },
    "owner": {
      "plugin": "users-permissions",
      "model": "user",
      "via": "reports"
    }
  }
}
